\"primary\" @click=\"flash1()\">上一页</el-button>\r\n      <el-button type=\"primary\" @click=\"flash()\">下一页</el-button>\r\n      </el-form-item>\r\n    </el-main>\r\n    </el-form>\r\n     <!-- <div><img src=\"\\images\\01.png\" alt=\"\"></div> -->\r\n      <!-- <el-button type=\"button\" @click=\"getResult()\">获取</el-button> -->\r\n      <!-- <p>{{form.test}}</p> -->\r\n      <!-- <p>{{test1}}</p> -->\r\n      <!-- <div>{{form.tactics}}</div> -->\r\n  </section>\r\n\r\n\r\n</template>\r\n<script src=\"https://unpkg.com/axios/dist/axios.min.js\"></script>\r\n<script src=\"https://cdn.jsdelivr.net/npm/vue/dist/vue.js\"></script>\r\n\r\n\r\n<script>\r\n\r\n\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      form: { //表单数据初始化\r\n        tactics: null,\r\n        desc: '111',\r\n        test:'test',\r\n        toolname:this.$route.query.name,\r\n        index:0\r\n      },\r\n       showClass: false  \r\n    };\r\n  },\r\ndirectives:{\r\n    trigger:{\r\n     inserted(el,binging){\r\n        el.click()\r\n       //$(el).trigger('click')\r\n      }\r\n   }\r\n    },\r\n  methods: {\r\n    next(){\r\n    this.$router.push({path: '/Recommended vulnerability scanning methods'})\r\n    },\r\n    tool(){\r\n    this.$router.push({path: '/GetInformationTool'})\r\n    },\r\n    flash(){\r\n      if (this.form.index<this.form.test.length-5){\r\n      this.form.index++;}\r\n      else{\r\n        alert('没有了');\r\n      }\r\n    },\r\n    flash1(){\r\n      if (this.form.index<this.form.test.length-4){\r\n      this.form.index=this.form.index-4;}\r\n      else{\r\n        alert('没有了');\r\n      }\r\n    },\r\n    goBack(){\r\n        this.$router.push({path: '/Recommended collection method' })},\r\n    getResult:function() {\r\n        var _this = this\r\n        _this.$axios.get('http://127.0.0.1:5000/api/tools/?name='+_this.form.toolname).then\r\n        (function(resp){\r\n          console.log(resp.data.result);\r\n          _this.form.test=resp.data.result;\r\n        },\r\n        function(err){})\r\n    },\r\n    onSubmit() {\r\n      let examDate = this.formatTime(this.form.examDate)\r\n      this.form.examDate = examDate.substr(0,10)\r\n      this.$axios(`/api/examManagePaperId`).then(res => {\r\n        this.form.paperId = res.data.data.paperId + 1 \r\n        this.$axios({\r\n          url: '/api/exam',\r\n          method: 'post', \r\n          data: {\r\n            ...this.form\r\n          }\r\n        }).then(res => {\r\n          if(res.data.code == 200) {\r\n            this.$message({\r\n              message: '数据添加成功',\r\n              type: 'success'\r\n            })\r\n            this.$router.push({path: '/selectExam'})\r\n          }\r\n        })\r\n      })\r\n    },\r\n    \r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.add {\r\n  padding: 0px 40px;\r\n  width: 600px;\r\n  box-shadow: 0 2px 4px rgba(0, 0, 0, .12), 0 0 6px rgba(0, 0, 0, .04);\r\n  height:800px;\r\n  position: absolute;\r\n  left: 50%;\r\n  top:50%;\r\n  transform: translate(-50%,-50%);\r\n\r\n}\r\n</style>\r\n\r\n"]}]}